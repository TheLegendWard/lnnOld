<?xml version='1.0' encoding='utf-8'?>
<widget id="com.imabuilder.thewardinnovationgroup.lnn" version="1.3.190316" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
	<name>LNN</name>
	<description>Liberia News Network LNN is an app that gets updated news from credible and reliable Liberian news websites.</description>
	<author email="linwilvic@gmail.com" href="http://twigllc.com/">Lincoln Ward</author>
	<content src="index.html" />
	<!-- Network Request Whitelist -->
	<access origin="*" />
	<platform name="android">
		<access origin="market:*" launch-external="yes"/>
		<access origin="tel:*" launch-external="yes"/>
		<access origin="geo:*" launch-external="yes"/>
		<access origin="mailto:*" launch-external="yes"/>
		<access origin="sms:*" launch-external="yes"/>
		<access origin="whatsapp:*" launch-external="yes"/>
		<access origin="line:*" launch-external="yes"/>
		<access origin="twitter:*" launch-external="yes"/>
		<access origin="fb:*" launch-external="yes"/>
		<access origin="fbapi20130214:*" launch-external="yes"/>
		<access origin="skype:*" launch-external="yes"/>
	</platform>
	<!-- Navigation Whitelist -->
	<allow-navigation href="http://*/*" />
	<allow-navigation href="https://*/*" />
	<allow-navigation href="data:*" />
	<allow-navigation href="file:*" />
	<allow-navigation href="http://localhost:8080/*"/>
	<!-- Intent Whitelist -->
	<allow-intent href="http://*/*" />
	<allow-intent href="https://*/*" />
	<allow-intent href="rtsp://*" />
	<allow-intent href="rtmp://*" />
	<allow-intent href="rtp://*" />
	<allow-intent href="udp://*" />
	<allow-intent href="file://*" />
	<allow-intent href="tel:*" />
	<allow-intent href="sms:*" />
	<allow-intent href="mms:*" />
	<allow-intent href="mailto:*" />
	<allow-intent href="geo:*" />
	<allow-intent href="google.navigation:*" />
	<allow-intent href="google.streetview:*" />
	<allow-intent href="maps:*" />
	<allow-intent href="map:*" />
	<allow-intent href="googlemap:*" />
	<allow-intent href="whatsapp:*" />
	<allow-intent href="line:*" />
	<allow-intent href="twitter:*" />
	<allow-intent href="fb:*" />
	<allow-intent href="fbapi20130214:*" />
	<allow-intent href="skype:*" />
	<allow-intent href="linkedin:*" />
	<allow-intent href="googlegmail:*" />
	<allow-intent href="youtube:*" />
	<platform name="android">
		<allow-intent href="market:*" />
	</platform>
	<platform name="ios">
		<allow-intent href="itms:*" />
		<allow-intent href="itms-apps:*" />
	</platform>
	<preference name="webviewbounce" value="false"/>
	<preference name="UIWebViewBounce" value="false"/>
	<preference name="DisallowOverscroll" value="true"/>
	<preference name="SplashScreenDelay" value="500"/>
	<preference name="FadeSplashScreenDuration" value="0"/>
	<preference name="android-minSdkVersion" value="19"/>
	<preference name="BackupWebStorage" value="none"/>
	<preference name="SplashScreen" value="screen"/>
	<preference name="StatusBarOverlaysWebView" value="false" />
	<preference name="StatusBarStyle" value="lightcontent" />
	<preference name="StatusBarBackgroundColor" value="#dddddd" />
	<platform name="android">
		<preference name="loadUrlTimeoutValue" value="20000" />
		<preference name="ErrorUrl" value="file:///android_asset/www/retry.html" />
		<preference name="LoadingDialog" value="Start...,Please wait..." />
	</platform>
	<platform name="ios">
		<preference name="ErrorUrl" value="Application Error" />
	</platform>
	<feature name="StatusBar">
		<param name="ios-package" onload="true" value="CDVStatusBar"/>
	</feature>
	<!-- default -->
	<plugin name="cordova-plugin-device" />
	<plugin name="cordova-plugin-console" />
	<plugin name="cordova-plugin-whitelist" />
	<plugin name="cordova-plugin-splashscreen" />
	<plugin name="cordova-plugin-statusbar" />
	<plugin name="ionic-plugin-keyboard" />
	<!-- ./default -->
	<!-- cordova-plugin-admob-free -->
	<plugin name="cordova-plugin-admob-free" spec="0.20.0" >
		<variable name="ADMOB_APP_ID" value="ca-app-pub-3103082555954281~4607146420" />
	</plugin>
	<!-- ./cordova-plugin-admob-free -->
	<plugin name="cordova-admob-sdk" />
	<!-- cordova-plugin-admob-free -->
	<plugin name="cordova-plugin-admob-free" spec="0.20.0" >
		<variable name="ADMOB_APP_ID" value="ca-app-pub-3103082555954281~4607146420" />
	</plugin>
	<!-- ./cordova-plugin-admob-free -->
	<plugin name="cordova-plugin-dialogs" />
	<plugin name="cordova-plugin-x-socialsharing" />
	<plugin name="cordova-universal-links-plugin" />
	<plugin name="cordova-disable-http-cache" />
	<plugin name="cordova-plugin-inappbrowser" />
	<gap:plugin name="onesignal-cordova-plugin" spec="2.4.5" source="npm" />
	<plugin name="cordova-promise-polyfill" />
	<platform name="android">
		<icon qualifier="ldpi" src="www/res/icon/android/ldpi-icon.png" />
		<icon qualifier="mdpi" src="www/res/icon/android/mdpi-icon.png" />
